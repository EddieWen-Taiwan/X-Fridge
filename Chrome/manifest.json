{
	"manifest_version": 2,

	"name": "X-Fridge",
	"description": "A Chrome / Firefox / Safari extension for food management.",
	"version": "1.0",

	"browser_action": {
		"default_icon": "favicon.png",
		"default_popup": "popup.html"
	},

	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},

	"permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"https://code.jquery.com/"
  ]
}